{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Swagger Client",
    "version": "0.0.0"
  },
  "tauri": {
    "updater": {
      "active": false,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDI0QkQxQ0IwQTJCOTgxNDMKUldSRGdibWlzQnk5SkF5ME4rNEVWMmVyL0ptNUdYZGNrMlUwTXZOeFl3UEYyaUVKR1p5NHFud0wK",
      "endpoints": [],
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      }
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": [
          "**"
        ]
      },
      "http": {
        "all": true,
        "request": true,
        "scope": ["https://**", "http://**"]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "swagger-client",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Swagger Client",
        "width": 1300,
        "height": 760
      }
    ]
  }
}
